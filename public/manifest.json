{
	"manifest_version": 3,
	"version": "1.1",
	"short_name": "AQI Levels Checker ",
	"name": "AQI  Quality Levels Checker",
	"action": {
		"default_popup": "index.html",
		"default_icon": "icon.png"
	},
	"permissions": ["tabs"],
	"icons": [
		{
			"src": "favicon.ico",
			"sizes": "64x64 32x32 24x24 16x16",
			"type": "image/x-icon"
		},
		{
			"src": "favicon.ico",
			"type": "image/png",
			"sizes": "192x192"
		},
		{
			"src": "favicon.ico",
			"type": "image/png",
			"sizes": "512x512"
		}
	],
	"start_url": ".",
	"display": "standalone",
	"theme_color": "#000000",
	"background_color": "#ffffff"
}
